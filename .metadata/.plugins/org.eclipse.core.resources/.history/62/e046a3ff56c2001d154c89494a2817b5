package com.harikrashna.springdemo.entity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="mobile")
public class Mobile {
	
	//define fields 
	private int id;
	private String phone_no;
//	private int user_id;
	public Mobile() {
		
	}
	
	public Mobile(String phone_no) {
		this.phone_no = phone_no;
	}
	
	
	
	
}
